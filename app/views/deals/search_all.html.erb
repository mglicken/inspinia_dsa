          <tr>
            <td>Deals</td>
            <td>
              <% if @deals.count == 0 %>
                <%= @deals.count %>
              <% elsif @deals.count < 11 %>
                <% Deal.where(id: @deals).order("name ASC").each do |deal| %>
                  <a href="/deals/<%= deal.id %>" ><%= deal.name %></a><br>
                <% end %>                
              <%else%>
                <% i=1 %>
                <% Deal.where(id: @deals).order("name ASC").each do |deal| %>
                  <a href="/deals/<%= deal.id %>" ><%= deal.name %></a><br>
                  <% i=i+1 %>
                  <%if i == 10 %>
                    <% break %>
                  <%end%>
                <% end %> 
                <strong><%= @deals.count-10 %> Others</strong>
              <%end%>
            </td>
            <% if @deals.count > 0 %> 
              <td><a class="btn btn-lincoln" href="/deal_search/<%= @text %>" role="button">View Results</a></td>
            <% else %>
              <td></td>
            <% end %>
