<br>
<div class="row">
  <div class="col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8">
    <div class="ibox-content">
      <div class="ibox-heading">
        <h3 class="ibox-title"> Engagement Sponsor # <%= @engagement_sponsor.id %></h3>
      </div>
      <div class="ibox-body">
        <div class="row">
          <div class="col-md-12">
            <dl>

              <dt> Sponsor ID</dt>
              <dd><%= @engagement_sponsor.sponsor_id %></dd>
              <dt> Deal ID</dt>
              <dd><%= @engagement_sponsor.deal_id %></dd>  

            </dl>
            <div class="btn-group btn-group-justified">
              <a href="/engagement_sponsors/" class="btn btn-primary">
                <i class="fa fa-reply"></i>
              </a>

              <a href="/engagement_sponsors/<%= @engagement_sponsor.id %>/edit" class="btn btn-warning">
                <i class="fa fa-edit"></i>
              </a>

            </div>
          </div>
        </div>
      </div>

    </div>
  </div>
</div>